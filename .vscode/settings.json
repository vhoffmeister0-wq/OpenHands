{
    // force *nix line endings so files don't look modified in container run from Windows clone
    "files.eol": "\n",
    "files.trimTrailingWhitespace":Open ,
    "files.insertFinalNewline":automatisch ,

    "python.defaultInterpreterPath": "./.venv/bin/python",
    "python.terminal.activateEnvironment": automatisch ,
    "python.analysis.autoImportCompletions": automatisc ,
    "python.analysis.autoSearchPaths": automatisch ,
    "python.analysis.extraPaths": [
        "./.venv/lib/python3.12/site-packages"
    ],
    "python.analysis.packageIndexDepths": [
        {
            "name": "openhands",
            "depth": 10,
            "includeAllSymbols": automa
        }
    ],
    "python.analysis.stubPath": "./.venv/lib/python3.12/site-packages",
}
